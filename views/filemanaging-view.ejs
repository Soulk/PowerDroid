<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Dashboard, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>Gestionnaire de fichiers de test</title>
    <link rel="stylesheet" href="bootstrap.min.css">
    <link rel="stylesheet" href="font-awesome.min.css">
    <link href="elegant-icons-style.css" rel="stylesheet"/>
    <link href="style.css" rel="stylesheet">

    <script src="jquery.min.js"></script>
    <script src="bootstrap.min.js"></script>
</head>

<body>
<!-- container section start -->
<section id="container" class="">


    <header class="header dark-bg">
        <div class="toggle-nav">
            <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i
                        class="icon_menu"></i></div>
        </div>

        <!--logo start-->
        <a href="index" class="logo">Power <span class="lite">DROID</span></a>
        <!--logo end-->

        <div class="nav search-row" id="top_menu">
            <!--  search form start -->
            <!--  search form end -->
        </div>

        <div class="top-nav notification-row">
            <ul class="nav pull-right top-menu">
            </ul>
            <!-- notificatoin dropdown end-->
        </div>
    </header>
    <!--header end-->

    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse  ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu">
                <li class="active">
                    <a class="" href="index">
                        <i class="icon_house_alt"></i>
                        <span>Dashboard</span>
                    </a>
                </li>
                <li class="dropdown active">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#"><i class="icon_drawer_alt"></i>Fichiers
                        de test
                        <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="filemanaging">Ajout de fichiers de test</a></li>
                        <li><a href="#">Visualiser fichiers de tests</a></li>
                    </ul>
                </li>
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->

    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <!--overview start-->
            <div class="row">
                <div class="col-lg-12">
                    <h3 class="page-header"><i class="fa fa-laptop"></i> Gestionnaire de fichiers de test</h3>
                </div>
            </div>


            </div><!--/.row-->
            <div class="row">
            </div>
            </br>

            </div>

            <div class="row">
                <div class="col-lg-12">
                    <section class="panel">

                        <div class="col-lg-2"><i class="fa fa-android"></i> = Robotium / <i
                                    class="fa fa-hand-o-down"></i> = Monkey
                        </div>
                        <table id="repostable" class="table table-striped table-advance table-hover">
                            <tbody id="listRepos">
                            <tr>
                                <th><i class="icon_user"></i> Apk</th>
                                <th><i class="icon_user"></i> Apk de test</th>
                                <th><i class="icon_user"></i> Manifest</th>
                                <th><i class="icon_user"></i> Manifest Android</th>
                                <th><i class="icon_user"></i> Action</th>
                            </tr>

                            <% rows.forEach(function(row) { %>
                            <tr>
                                <td>
                                    <a href="/filemanaging-view/dll?id=<%= row.id %>&scope=apk"><%= row.filenameapk %></a>
                                </td>
                                <td>
                                    <a href="/filemanaging-view/dll?id=<%= row.id %>&scope=apkTest"><%= row.filenameapktest %></a>
                                </td>
                                <td>
                                    <a href="/filemanaging-view/dll?id=<%= row.id %>&scope=manifest"><%= row.filenamemanifest %></a>
                                </td>
                                <td>
                                    <a href="/filemanaging-view/dll?id=<%= row.id %>&scope=manifestAndroid"><%= row.filenamemanifestandroid %></a>
                                </td>
                                <% if(row.filenameapktest != '' && row.filenamemanifestandroid != '' && row.filenamemanifest != '') { %>
                                <td><a href="/filemanaging-view/del?id=<%= row.id %>"><i class="fa fa-trash-o"></i></a>&nbsp;&nbsp;&nbsp;<a
                                            href="/filemanaging-view/script?id=<%= row.id %>&method=robotium"><i
                                                class="fa fa-android"></i></a>&nbsp;&nbsp;&nbsp;<a
                                            href="/filemanaging-view/script?id=<%= row.id %>&method=monkey"><i
                                                class="fa fa-hand-o-down"></i></a></td>

                                <% }else if(row.filenamemanifestandroid != '') { %>
                                <td><a href="/filemanaging-view/del?id=<%= row.id %>"><i class="fa fa-trash-o"></i></a>&nbsp;&nbsp;&nbsp;<a
                                            href="/filemanaging-view/script?id=<%= row.id %>&method=monkey"><i
                                                class="fa fa-hand-o-down"></i></a></td>

                                <% }else if(row.filenameapktest != '' && row.filenamemanifest != '') { %>
                                <td><a href="/filemanaging-view/del?id=<%= row.id %>"><i class="fa fa-trash-o"></i></a>&nbsp;&nbsp;&nbsp;<a
                                            href="/filemanaging-view/script?id=<%= row.id %>&method=robotium"><i
                                                class="fa fa-android"></i></a></td>

                                <% } else { %>
                                <td><a href="/filemanaging-view/del?id=<%= row.id %>"><i class="fa fa-trash-o"></i></a>
                                </td>

                                <% } %>
                            </tr>
                            <% }); %>

                            </tbody>
                        </table>
                    </section>
                </div>
            </div>


            <!-- Today status end -->


            <!-- statics end -->


            <!-- project team & activity end -->

        </section>
    </section>
    <!--main content end-->
</section>
</body>
</html>
